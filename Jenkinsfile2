pipeline {

  agent any

  stages {

    stage('Checkout Source') {
      steps {
        git url:'https://github.com/sachinj77/myapp1.git', branch:'master'
      }
    }
    
      stage("Build image") {
            steps {
                script {
                    myapp = docker.build("sachinjp/hellowhale:${env.BUILD_ID}")
                }
            }
        }
      } 
    }  
